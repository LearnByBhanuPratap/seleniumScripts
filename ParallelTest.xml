<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="LoginTest" parallel="tests" thread-count="2" preserve-order="true">

	<test name="ParallelTest1 Browser Chrome">
		<parameter name="myBrowser" value="chrome" />
		<classes>
			<class name="com.test.selenium.basciscripts.ParallelTest" />
		</classes>
	</test> <!-- Test -->

	<test name="ParallelTest2 Browser Firefox">
		<parameter name="myBrowser" value="firefox" />
		<classes>
			<class name="com.test.selenium.basciscripts.ParallelTest" />
		</classes>
	</test> <!-- Test -->
	
	<test name="ParallelTest3 Browser Chrome">
		<parameter name="myBrowser" value="chrome" />
		<classes>
			<class name="com.test.selenium.basciscripts.ParallelTest" />
		</classes>
	</test> <!-- Test -->
	
	<test name="ParallelTest4 Browser Firefox">
		<parameter name="myBrowser" value="firefox" />
		<classes>
			<class name="com.test.selenium.basciscripts.ParallelTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
